{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RedisConfig": {
    "Port": 6379,
    "Host": "redis",
    "AbsoluteExpirationMinutes": 10,
    "SlidingExpirationMinutes": 10
  },
  "Kestrel": {
    "Endpoints": {
      "grpc": {
        "Url": "https://recordhub.catalogservice.api:5005",
        "Protocols": "Http2"
      },
      "webApi": {
        "Url": "https://recordhub.catalogservice.api:5004",
        "Protocols": "Http1"
      }
    }
  },
  "ElasticsearchConfig": {
    "Index": "records",
    "Url": "http://elasticsearch:9200/"
  }
}
