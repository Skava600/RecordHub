{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "AppLoggingSettings": {
    "PostgresSettings": {
      "TableName": "Serilog",
      "Schema": "public"
    }
  },
  "ElasticsearchConfig": {
    "Index": "records",
    "Url": "http://elasticsearch:9200/"
  },
  "Kestrel": {
    "Endpoints": {
      "grpc": {
        "Url": "https://recordhub.catalogservice.api:5005",
        "Protocols": "Http2"
      },
      "webApi": {
        "Url": "https://recordhub.catalogservice.api:5004",
        "Protocols": "Http1"
      }
    }
  }
}
