{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppLoggingSettings": {
    "PostgresSettings": {
      "TableName": "Serilog",
      "Schema": "public"
    },
    "General": {
      "RestrictedToMinimumLevel": "Information"
    }
  },

  "Identity": {
    "DataProtectionTokenLifeSpanHours": 2,
    "EmailConfirmationTokenLifeSpanDays": 3
  }
}
