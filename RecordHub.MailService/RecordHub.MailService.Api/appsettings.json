{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Enrich": [ "WithHangfireContext" ],
    "WriteTo": [
      {
        "Name": "Hangfire",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  },
  "MassTransit": {
    "Host": "rabbitmq://messagebus",
    "EmailQueue": "send-email"
  }
}
